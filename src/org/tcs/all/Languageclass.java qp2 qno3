public class Languageclass {
	public void alllanguage() {
		// TODO Auto-generated method stub
System.out.println("alllanguage");
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Languageclass a = new Languageclass();
		a.alllanguage();
